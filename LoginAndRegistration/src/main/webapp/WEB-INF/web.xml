<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>registration-form</servlet-name>
    <servlet-class>com.loginPage.newForm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>registration-form</servlet-name>
    <url-pattern>/registration-form</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>servlet1</servlet-name>
    <servlet-class>com.loginPage.homepage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servlet1</servlet-name>
    <url-pattern>/get</url-pattern>
  </servlet-mapping>

 <!-- <servlet>
    <servlet-name>servlet2</servlet-name>
    <servlet-class>com.serveletfilter.serveletFilter2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servlet2</servlet-name>
    <url-pattern>/second</url-pattern>
  </servlet-mapping>-->

  <servlet>
    <servlet-name>displayUsers</servlet-name>
    <servlet-class>com.loginPage.displayUsers</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>displayUsers</servlet-name>
    <url-pattern>/third</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>newUser</servlet-name>
    <servlet-class>com.loginPage.newUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>newUser</servlet-name>
    <url-pattern>/fourth</url-pattern>
  </servlet-mapping>

  <filter>
    <filter-name>filter1</filter-name>
    <filter-class>com.loginPage.filter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>filter1</filter-name>
    <url-pattern>/get</url-pattern>
  </filter-mapping>
